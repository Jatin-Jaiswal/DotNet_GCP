# Backend Environment Configuration Example
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# ======================================
# Cloud SQL PostgreSQL Configuration
# ======================================
# Private IP of your Cloud SQL PostgreSQL instance
CLOUDSQL_HOST=10.92.160.3
CLOUDSQL_DATABASE=dotnetdb
CLOUDSQL_USERNAME=postgres
CLOUDSQL_PASSWORD=postgres

# ======================================
# Memorystore Redis Configuration
# ======================================
# Private IP of your Memorystore Redis instance
REDIS_HOST=10.127.80.5
REDIS_PORT=6379

# ======================================
# GCP Project Configuration
# ======================================
# Your GCP project ID
GCP_PROJECT_ID=project-84d8bfc9-cd8e-4b3c-b15

# ======================================
# Pub/Sub Configuration
# ======================================
# Name of your Pub/Sub topic
PUBSUB_TOPIC_ID=dot-net-topic

# ======================================
# Cloud Storage Configuration
# ======================================
# Name of your Cloud Storage bucket
STORAGE_BUCKET_NAME=dot-net-bucket

# ======================================
# ASP.NET Core Configuration
# ======================================
# URL and port the application listens on
ASPNETCORE_URLS=http://+:8080
# Development or Production
ASPNETCORE_ENVIRONMENT=Development
